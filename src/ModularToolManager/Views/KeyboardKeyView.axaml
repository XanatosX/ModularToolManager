<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ModularToolManager.Views.KeyboardKeyView">
	<StackPanel Orientation="Horizontal" Spacing="5">
		<TextBlock VerticalAlignment="Center" IsEnabled="{Binding PrefixSet}" Text="{Binding PrefixText}"/>
		<Button IsEnabled="False" Content="{Binding ButtonText}"/>
		<TextBlock VerticalAlignment="Center" IsEnabled="{Binding PostfixSet}" Text="{Binding PostfixText}"/>
	</StackPanel>
</UserControl>
